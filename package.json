{
  "name": "code-challenge-store",
  "version": "2.0.1",
  "description": "Code challenge",
  "engineStrict": true,
  "engines": {
    "node": ">=6.9"
  },
  "devDependencies": {
    "babel-cli": "~6.24.1",
    "babel-core": "~6.25.0",
    "babel-eslint": "~7.2.2",
    "babel-jest": "~19.0.0",
    "babel-plugin-transform-async-to-generator": "~6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.24.1",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-stage-3": "~6.24.1",
    "eslint": "~3.19.0",
    "eslint-config-airbnb-base": "~11.1.3",
    "eslint-plugin-flowtype": "~2.31.0",
    "eslint-plugin-import": "~2.2.0",
    "jest": "~19.0.2"
  },
  "scripts": {
    "clean": "rm -rf coverage dist tmp",
    "lint": "eslint .",
    "start": "node dist/src/main.js",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "babel src/**/*.js -d dist",
    "build:watch": "babel --watch src/**/*.js -d dist"
  },
  "author": "Gabriel Garc√≠a Seco <ggarciaseco@gmail.com> ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielseco/code-challenge-store.git"
  },
  "bugs": {
    "url": "https://github.com/jsynowiec/code-challenge-store/issues"
  },
  "homepage": "https://github.com/jsynowiec/code-challenge-store#readme",
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "babel-runtime": "~6.23.0",
    "chalk": "^2.1.0"
  }
}
